{
  "name": "puppet-ghost",
  "version": "1.1.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Install and Manage Ghost Blogs",
  "license": "AGPL-3.0",
  "source": "https://github.com/voxpupuli/puppet-ghost.git",
  "project_page": "https://github.com/voxpupuli/puppet-ghost",
  "issues_url": "https://github.com/voxpupuli/puppet-ghost/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7.0",
        "6.0"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    },
    {
      "name": "puppet/nodejs",
      "version_requirement": ">= 1.3.0 < 2.0.0"
    },
    {
      "name": "proletaryo/supervisor",
      "version_requirement": ">= 0.5.3 < 2.0.0"
    }
  ]
}
